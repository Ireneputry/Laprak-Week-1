     "text": [
      "Waktu yang dibutuhkan Erika untuk menabung adalah 8 tahun\n"
     ]
    }
   ],
   "source": [
    "uang_awal = 200000000\n",
    "target    = 400000000\n",
    "bunga = 10/100 \n",
    "\n",
    "tahun_menabung = 0\n",
    "\n",
    "while uang_awal < target :\n",
    "    uang_awal += uang_awal * bunga\n",
    "    tahun_menabung += 1\n",
    "\n",
    "print(f\"Waktu yang dibutuhkan Erika untuk menabung adalah {tahun_menabung} tahun\")"
   ]
